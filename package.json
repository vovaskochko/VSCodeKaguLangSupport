{
  "name": "kaguasmlang",
  "displayName": "KaguOS Assembler Language Support",
  "description": "Support of KaguOS assembler language",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "kaguasm",
        "aliases": [
          "KaguOS Assembler",
          "kaguasm"
        ],
        "extensions": [
          ".kga"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "kaguasm",
        "scopeName": "source.kga",
        "path": "./syntaxes/kaguasm.tmLanguage.json"
      }
    ]
  }
}
